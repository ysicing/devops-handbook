{
  "name": "blog",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "lint": "yarn lint:js && yarn lint:style",
    "lint:js": "eslint --fix --report-unused-disable-directives \"**/*.{js,jsx,ts,tsx,mjs}\"",
    "lint:style": "stylelint \"**/*.css\"",
    "lint:fix": "eslint src --fix",
    "prettier:fix": "npx prettier src --check --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@dipakparmar/docusaurus-plugin-umami": "^1.0.2",
    "@docusaurus/core": "^2.0.1",
    "@docusaurus/plugin-debug": "^2.0.1",
    "@docusaurus/plugin-ideal-image": "^2.0.1",
    "@docusaurus/plugin-pwa": "^2.0.1",
    "@docusaurus/preset-classic": "^2.0.1",
    "@docusaurus/theme-live-codeblock": "^2.0.1",
    "@easyops-cn/docusaurus-search-local": "^0.31.0",
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@giscus/react": "^2.0.6",
    "@popperjs/core": "^2.11.2",
    "dayjs": "^1.10.7",
    "docusaurus-plugin-image-zoom": "^0.1.1",
    "docusaurus-plugin-matomo": "^0.0.6",
    "ora": "^5.3.0",
    "raw-loader": "^4.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-popper": "^2.2.5",
    "react-spring": "^8.0.27",
    "react-tooltip": "^4.2.21"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@docusaurus/eslint-plugin": "^2.0.1",
    "@docusaurus/module-type-aliases": "^2.0.1",
    "@tsconfig/docusaurus": "^1.0.5",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^26.0.0",
    "typescript": "^4.7.4"
  }
}
